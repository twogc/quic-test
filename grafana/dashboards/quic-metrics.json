{
  "dashboard": {
    "id": null,
    "title": "2GC Network Protocol Suite - QUIC Metrics",
    "tags": ["quic", "network", "2gc"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "QUIC Server Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "quic_server_connections_total",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "QUIC Server Bytes Total",
        "type": "stat",
        "targets": [
          {
            "expr": "quic_server_bytes_total",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": {
              "mode": "thresholds"
            }
          }
        }
      },
      {
        "id": 3,
        "title": "QUIC Server Streams",
        "type": "stat",
        "targets": [
          {
            "expr": "quic_server_streams_total",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "QUIC Server Errors",
        "type": "stat",
        "targets": [
          {
            "expr": "quic_server_errors_total",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 10
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "QUIC Server Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "quic_server_uptime_seconds",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            }
          }
        }
      },
      {
        "id": 6,
        "title": "QUIC Bytes Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "quic_server_bytes_total",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
